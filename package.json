{
  "name": "gfg-gpe-frontend",
  "version": "0.1.12",
  "private": true,
  "homepage": "/",
  "dependencies": {
    "history": "^4.5.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "enzyme": "^2.7.1",
    "eslint": "^3.17.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "^6.10.0",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.0",
    "npm-run-all": "^4.0.2",
    "pre-commit": "^1.1.2",
    "react-addons-test-utils": "^15.4.2",
    "react-scripts": "0.9.3",
    "wdio-jasmine-framework": "^0.2.19",
    "webdriverio": "^4.6.2"
  },
  "scripts": {
    "build-css": "node-sass src/ -o src/",
    "watch-css": "npm run build-css && node-sass src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "code-coverage": "npm test -- --coverage --collectCoverageFrom=src/**/*js --collectCoverageFrom=!src/containers/routers/*js --collectCoverageFrom=!src/index.js",
    "test": "react-scripts test --env=jsdom",
    "front-end-tests": "wdio wdio.conf.js",
    "eject": "react-scripts eject",
    "eslint": "sh hook/pre-commit.sh"
  },
  "pre-commit": [
    "eslint"
  ]
}
